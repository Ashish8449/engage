  <% if(messages.success_msg) { %>
    <div class="alert alert-success alert-dismissible fade show mb-0 pb-0"
      role="alert">
      <strong>
        <%= messages.success_msg %>
      </strong>
      <button type="button" class="btn-close" data-bs-dismiss="alert"
        aria-label="Close"></button>
      </button>
    </div>
    <% } %>

      <% if (messages.error_msg) { %>
        <div class="alert alert-danger alert-dismissible fade show  mb-0  pb-0"
          role="alert">
          <strong>
            <%= messages.error_msg%>
          </strong>

          <button type="button" class="btn-close" data-bs-dismiss="alert"
            aria-label="Close"></button>



        </div>
        <% } %>

